{
    "manifest_version": 2,

    "name": "One-click Flickr",
    "description": "Browser action example with Flickr API!",
    "version": "1.0",

    "permissions": [
        "https://secure.flickr.com/"
    ],
    "browser_action": {
        "default_icon": "flickr_icon.png",
        "default_popup": "popup.html"
    }
}
